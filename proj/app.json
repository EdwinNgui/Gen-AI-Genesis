    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiZGFuaWVsbHUiLCJhIjoiY2x1ZWQ1NWMzMXI4ZDJzbGQxNjd4bThxaiJ9.UK_KZcIL9-iN9QcdRYKF3g",
